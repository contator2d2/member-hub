# Configurações do servidor
NODE_ENV=production
PORT=3000

# Banco de dados PostgreSQL
DATABASE_URL=postgresql://postgres:sua_senha@postgres:5432/area_membros

# JWT
JWT_SECRET=gere_um_secret_seguro_de_256_bits
JWT_EXPIRES_IN=7d

# CORS - URL do frontend
CORS_ORIGIN=https://seu-frontend.lovable.app

# Upload de arquivos (opcional)
UPLOAD_DIR=./uploads
MAX_FILE_SIZE=52428800
